@using MvcStok.Models.Entity
@model List<Tbl_Satislar>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Ürün</th>
        <th>Müşteri</th>
        <th>Adet</th>
        <th>Tarih</th>

    </tr>
    <tbody>
        @foreach (var sts in Model)
        {
            <tr>
                <th>@sts.SatisId</th>
				<td>@sts.Tbl_Urunler.UrunMarka @sts.Tbl_Urunler.UrunAd</td>
                <td>@sts.Tbl_Musteri.MusteriAd @sts.Tbl_Musteri.MusteriSoyad</td>
                <td>@sts.Adet</td>
                <td>@sts.Tarih</td>
            </tr>
        }
    </tbody>
</table>

